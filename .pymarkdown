{
  "mode": {
    "strict-config": false
  },
  "plugins": {
    "md013": {
      "enabled": true,
      "line_length": 120,
      "code_blocks": false,
      "tables": false,
      "headings": true,
      "headers": true
    },
    "md030": {
      "enabled": true,
      "ul_single": 1,
      "ol_single": 1,
      "ul_multi": 1,
      "ol_multi": 1
    },
    "md029": {
      "enabled": true,
      "style": "ordered"
    },
    "md004": {
      "enabled": true,
      "style": "dash"
    },
    "md041": {
      "enabled": false
    }
  },
  "extensions": {
    "front-matter": {
      "enabled": true
    }
  }
}
